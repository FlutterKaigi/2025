{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "stripe-webhook",
  "main": "src/index.ts",
  "compatibility_date": "2025-07-15",
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  },
  "env": {
    "staging": {
      "vars": {
        "ENVIRONMENT": "staging"
      }
    },
    "production": {
      "vars": {
        "ENVIRONMENT": "production"
      }
    }
  }
}
