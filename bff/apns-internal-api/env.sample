# APNs Internal API 環境変数サンプル
# 使用方法: このファイルを .env にコピーして、実際の値を設定してください
# cp env.sample .env
# source .env

# API 認証キー（任意の文字列を設定）
export X_API_KEY="your_test_api_key_here"

# APNs 認証情報（Apple Developer Portal から取得）
# https://developer.apple.com/account/resources/authkeys/list

# APNs キー ID（10文字）
export APNS_KEY_ID="ABC123DEFG"

# Apple Team ID（10文字）
export APNS_TEAM_ID="XYZ789HIJK"

# APNs 秘密鍵（.p8 ファイルの内容をそのまま貼り付け）
export APNS_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----
MIGTAgEAMBMGByqGSM49AgEGCCqGSM49AwEHBHkwdwIBAQQg...
（ここに実際の秘密鍵の内容を貼り付けてください）
...
-----END PRIVATE KEY-----"

# APNs 環境（sandbox または production）
export APNS_ENVIRONMENT="sandbox"

# ログレベル（trace, debug, info, notice, warning, error, critical）
export LOG_LEVEL="debug"

# サーバーポート（デフォルト: 8080）
export PORT="8080"
