{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "app-catalog-debug",
      "cwd": "apps/app_catalog",
      "request": "launch",
      "program": "lib/main.dart",
      "type": "dart"
    },
    {
      "name": "app-development-debug",
      "cwd": "apps/app",
      "request": "launch",
      "program": "lib/main.dart",
      "type": "dart",
      "args": [
        "--dart-define-from-file",
        "environments/.env.dev",
        "--web-experimental-hot-reload",
        "--web-port",
        "3000"
      ]
    },
    {
      "name": "app-staging-debug",
      "cwd": "apps/app",
      "request": "launch",
      "program": "lib/main.dart",
      "type": "dart",
      "args": [
        "--dart-define-from-file",
        "environments/.env.stg",
        "--web-experimental-hot-reload",
        "--web-port",
        "3000"
      ]
    },
    {
      "name": "app-production-debug",
      "cwd": "apps/app",
      "request": "launch",
      "program": "lib/main.dart",
      "type": "dart",
      "args": [
        "--dart-define-from-file",
        "environments/.env.prod",
        "--web-experimental-hot-reload",
        "--web-port",
        "3000"
      ]
    }
  ]
}
